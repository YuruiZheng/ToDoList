<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TreeItem?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.*?>
<GridPane prefWidth="150"
          style="-fx-background: rgba(52, 52, 52, 1); -fx-border-color: rgba(52, 52, 52, 1); -fx-border-width: 1;-fx-border-color: rgba(52, 52, 52, 1) rgba(52, 52, 52, 1) rgba(52, 52, 52, 1) #ffa800;-fx-border-style: solid;"
          xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="client.scenes.MyBoardsCtrl">
    <ScrollPane fx:id="treeContainer" fitToHeight="true" prefWidth="150" GridPane.rowIndex="1">
        <TreeView prefHeight="0" prefWidth="150" fx:id="treeView" styleClass="myTree" stylesheets="@stylesheet.css">
            <TreeItem expanded="true" value="My Servers">
            </TreeItem>
        </TreeView>
    </ScrollPane>
    <Button fx:id="hideBoards" alignment="TOP_CENTER" contentDisplay="CENTER" minHeight="29.0" onAction="#hideBoards"
            prefHeight="29.0" prefWidth="148.0" style="-fx-background-color: rgba(52, 52, 52, 1);" styleClass="lulo"
            stylesheets="@stylesheet.css" text="Hide boards" textAlignment="CENTER" textFill="WHITE"
            GridPane.halignment="CENTER" GridPane.valignment="CENTER">
    </Button>
    <rowConstraints>
        <RowConstraints maxHeight="93.0" minHeight="36.0" prefHeight="36.0"/>
        <RowConstraints maxHeight="1000.0" minHeight="337.0"/>
    </rowConstraints>
</GridPane>
